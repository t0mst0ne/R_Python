# Removing NA values  using is.na
x <- c(1, 2, NA, 4, NA, 5)
bad <- is.na(x)
x[!bad] x[bad] bad
x <- c( 1, 2, NA, 3,NA, 5)
y <- c("a", "b", NA, "d", NA, "f")

# Using complete.cases
good <- complete.cases(x,y)
x[good]
y[good]

airquality[1:6,]
good <- complete.cases(airquality)
airquality[good, ][1:6, ]
